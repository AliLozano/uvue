{
  "name": "@uvue/rquery",
  "version": "0.4.0-alpha.0",
  "description": "Helper to traverse and manipulate AST with recast",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib && tsc",
    "watch": "tsc -w",
    "lint": "tslint 'src/**/*.ts'",
    "lint:fix": "tslint --fix 'src/**/*.ts'",
    "format": "prettier --ignore-path .gitignore --write 'src/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "recast",
    "query",
    "ast"
  ],
  "author": "Simon Babay <simon@babay.fr>",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.0.0",
    "@types/node": "^10.5.2",
    "lodash": "^4.17.10",
    "prettier": "^1.13.7",
    "recast": "^0.15.2",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0"
  }
}
